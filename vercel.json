{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/api/:match*", "destination": "/api/:match*" },
    { "source": "/:folder(css|js|images|assets|fonts)/:file*", "destination": "/:folder/:file*" },
    { "source": "/:path+", "destination": "/:path+.html" }
  ],
  "redirects": [
    { "source": "/:path+.html", "destination": "/:path+", "permanent": true },
    { "source": "/services/renovation-exterieurs", "destination": "/services/renovation-exterieur", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}